<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@c0426id/gradient_background">
        <shape>
            <gradient android:startColor="?attr/themeGradientStart" android:endColor="?attr/themeGradientEnd" android:angle="270" android:centerColor="?attr/themeGradientEnd"/>
        </shape>
    </item>
    <item android:id="@c0426id/gradient_background_night">
        <shape>
            <gradient android:startColor="@color/themeGradientEndNight" android:endColor="@color/themeGradientStartNight" android:angle="270" android:centerColor="@color/themeGradientStartNight"/>
        </shape>
    </item>
    <item android:id="@c0426id/background_overlay">
        <color android:color="@android:color/black"/>
    </item>
</layer-list>
