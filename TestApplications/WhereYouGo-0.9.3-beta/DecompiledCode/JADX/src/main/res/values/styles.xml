<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme">
        <item name="android:windowAnimationStyle">@null</item>
    </style>
    <style name="Mapsforge">
    </style>
    <style name="ThemeSettings">
    </style>
    <style name="zxing_CaptureTheme">
    </style>
    <style name="CustomScreenBottomInfo">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="CustomScreenTextTitle">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="CustomScreenTextTitleDark">
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="CustomScreenTextValue">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="CustomScreenTextValueDark">
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="FontSizeLarge">
        <item name="android:textSize">20sp</item>
    </style>
    <style name="FontSizeMedium">
        <item name="android:textSize">16sp</item>
    </style>
    <style name="FontSizeSmall">
        <item name="android:textSize">12sp</item>
    </style>
    <style name="MainScreenButton">
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:layout_gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:background">@drawable/selector_button</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="MainScreenLine">
        <item name="android:orientation">@null</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="Panel">
        <item name="android:gravity">clip_horizontal</item>
        <item name="android:orientation">@null</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/title_height</item>
    </style>
    <style name="PanelBottomAboveShadow">
        <item name="android:background">@drawable/var_bottom_shadow</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/shadow_height</item>
    </style>
    <style name="PanelBottomMain">
        <item name="android:id">@id/panel_bottom_main</item>
        <item name="android:background">@color/title_background</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="PanelButton">
        <item name="android:background">@drawable/selector_button</item>
        <item name="android:layout_width">@dimen/title_height</item>
        <item name="android:layout_height">@dimen/title_height</item>
    </style>
    <style name="PanelButton.Width">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="PanelToggleButton">
        <item name="android:background">@drawable/selector_toggle_button_v1</item>
        <item name="android:padding">@dimen/title_height_quarter</item>
        <item name="android:layout_width">@dimen/title_height</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textOn" />
        <item name="android:textOff" />
    </style>
    <style name="PanelTopBelowShadow">
        <item name="android:background">@drawable/var_top_shadow</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/shadow_height</item>
    </style>
    <style name="PanelTopMain">
        <item name="android:id">@id/panel_top_main</item>
        <item name="android:background">@color/title_background</item>
    </style>
    <style name="SeparatorHorizontal">
        <item name="android:background">@color/title_separator</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>
    <style name="SeparatorHorizontal.Dark">
        <item name="android:background">@color/title_separator_dark</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>
    <style name="SeparatorVertical">
        <item name="android:background">@color/title_separator</item>
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="SeparatorVertical.Dark">
        <item name="android:background">@color/title_separator_dark</item>
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="TextStyle">
        <item name="android:textStyle">@null</item>
        <item name="android:gravity">clip_horizontal</item>
    </style>
    <style name="TextStyle12">
        <item name="android:textSize">12sp</item>
    </style>
    <style name="TextStyle14">
        <item name="android:textSize">14sp</item>
    </style>
    <style name="TextStyle14B">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TextStyle16">
        <item name="android:textSize">16sp</item>
    </style>
    <style name="TextStyle16B">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TextStyle18">
        <item name="android:textSize">18sp</item>
    </style>
    <style name="TextStyle18B">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TextStyle20B">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TextStyle20BRed">
        <item name="android:textColor">#ffff0000</item>
    </style>
    <style name="TextStyleB">
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TitleProgressIndicator">
        <item name="android:gravity">clip_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">14dp</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:indeterminate">true</item>
    </style>
    <style name="TitleText">
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">clip_horizontal</item>
        <item name="android:id">@id/title_text</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1</item>
    </style>
</resources>
